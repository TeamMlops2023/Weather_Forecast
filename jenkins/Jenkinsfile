pipeline {
    agent any

    stages {
        stage('Checkout') {
            steps {
                git branch: 'BAS_ALAIN', url: 'https://github.com/TeamMlops2023/Weather_Forecast.git'
            }
        }

        stage('Build and Deploy') {
            steps {
                script {
                    // Assurez-vous que le chemin du répertoire est correct et que docker-compose.yml est à cet emplacement
                    sh 'docker-compose -f Weather_Forecast/docker-compose.yml up -d'
                }
            }
        }

        // Ajoutez d'autres étapes au besoin
    }
}
